page{ width: 100%; height:100%; }
map{ width: 100%; height:320px; position: fixed;top: 40px;z-index: 1;}


.main-bg {
  padding-top: 320px;
}

.header {
  background: rgb(230, 230, 230);
  height: 25px;
  line-height: 25px;
  padding-left: 30rpx;
  font-size: 13px;
  align-items: center;
  position: sticky;
  top: 320px;
}
.header2{
  background: rgb(230, 230, 230);
  height: 20px;
